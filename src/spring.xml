<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

   <bean id="conway" class="br.unb.cic.lp.gol.Conway">
       <constructor-arg index="0" type="int" value="10"/>
       <constructor-arg index="1" type="int" value="10"/>
       <constructor-arg ref="statistics"/>
   </bean>
   
   <bean id="highlife" class="br.unb.cic.lp.gol.HighLife">
       <constructor-arg index="0" type="int" value="10"/>
       <constructor-arg index="1" type="int" value="10"/>
       <constructor-arg ref="statistics"/>
   </bean>
   
   <bean id="livefreeordie" class="br.unb.cic.lp.gol.LiveFreeOrDie">
       <constructor-arg index="0" type="int" value="10"/>
       <constructor-arg index="1" type="int" value="10"/>
       <constructor-arg ref="statistics"/>
   </bean>
   
   <bean id="ameba" class="br.unb.cic.lp.gol.Ameba">
       <constructor-arg index="0" type="int" value="10"/>
       <constructor-arg index="1" type="int" value="10"/>
       <constructor-arg ref="statistics"/>
   </bean>
   
   <bean id="statistics" class="br.unb.cic.lp.gol.Statistics">
   </bean>

</beans>